[color]
  ui = auto
[alias]
  st      = status --short --branch
  co      = checkout
  out     = log FETCH_HEAD..HEAD
  in      = log HEAD..FETCH_HEAD
  ignored = ls-files --others -i --exclude-standard
  ffm     = merge --ff-only FETCH_HEAD
[diff]
  noprefix = true
  tool = vimdiff
[push]
  default = current
[core]
  excludesfile = ~/.gitignore
